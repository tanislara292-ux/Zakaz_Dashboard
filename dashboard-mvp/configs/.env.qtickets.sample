# QTickets API Configuration
# Скопируйте этот файл в secrets/.env.qtickets и заполните реальные значения

# API токен для доступа к QTickets API
QTICKETS_TOKEN=your_qtickets_api_token_here

# Количество дней для загрузки исторических данных (по умолчанию 30)
QTICKETS_DAYS_BACK=30

# URL API QTickets (обычно не меняется)
QTICKETS_API_URL=https://api.qtickets.ru/v1

# Таймаут запросов в секундах
QTICKETS_TIMEOUT=30

# Настройки логирования
LOG_LEVEL=INFO
LOG_JSON=false
LOG_FILE=logs/qtickets.log