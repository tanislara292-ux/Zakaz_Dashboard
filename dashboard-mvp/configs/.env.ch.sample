# ClickHouse Configuration
# Скопируйте этот файл в secrets/.env.ch и заполните реальные значения

# Хост сервера ClickHouse
CH_HOST=localhost

# Порт сервера ClickHouse
CH_PORT=8443

# Имя пользователя для подключения к ClickHouse
CH_USER=etl_writer

# Пароль для подключения к ClickHouse
CH_PASSWORD=your_clickhouse_password_here

# Имя базы данных
CH_DATABASE=zakaz

# Использовать HTTPS
CH_SECURE=true

# Настройки логирования
LOG_LEVEL=INFO
LOG_JSON=false
LOG_FILE=logs/clickhouse.log